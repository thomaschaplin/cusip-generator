{
    "name": "@thomaschaplin/cusip-generator",
    "version": "1.0.10",
    "description": "Generate a valid CUSIP",
    "main": "build/index.js",
    "files": [
        "build",
        "LICENSE",
        "readme.md",
        "assets/logo.png"
    ],
    "scripts": {
        "test": "jest --runInBand",
        "test:coverage": "rm -rf coverage && npm test -- --coverage",
        "test:mutation": "stryker run",
